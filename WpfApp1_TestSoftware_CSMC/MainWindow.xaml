        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1_TestSoftware_CSMC"
        mc:Ignorable="d"
        
        Title="无线测试上位机"
        Name="softWareMainWindow" 
        MaxHeight="600 " MaxWidth="800"
        MinHeight="600 " MinWidth="800">
    <Grid>
        <TabControl Name="tabControl" SelectionChanged="TabControl_SelectionChanged" >
            <TabItem Header="串口通信" MinHeight="10" MinWidth="20">
                <local:SerialBase Loaded="SerialBaseUserControl_Loaded"></local:SerialBase>

            </TabItem>
        </TabControl>

    </Grid>
</Window>
